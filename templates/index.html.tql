<!DOCTYPE html>
<html lang="ja">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <style>{{ .CSS }}</style>
  <title>OJ Portfolio</title>
</head>

<body>
  <header>
    <img class="header-logo" src="{{ .LogoURL }}" alt="logo">
  </header>
  <main>
    <div class="introduction">
      <div class="profile_img">
        <img src="{{ .ProfileURL }}" alt="profile">
      </div>
      <div class="profile_text">
        <p class="school">{{ .School }} {{ .Department }} {{ .Grade }}å¹´</p>
        <p class="name">{{ .Name }}</p>
        <p class="alp_name">{{ .AlpName }}</p>
      </div>
    </div>
    <div class="portfolio">
      {{ .HTML }}
    </div>
  </main>
</body>

</html>